<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endif -%}

    {%- unless settings.type_heading_font.system? and settings.type_body_font.system? -%}
      <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
      href="https://fonts.googleapis.com/css2?family=Maven+Pro:wght@400..900&family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap"
      rel="stylesheet"
    >
    
    <link
      href="https://fonts.googleapis.com/css2?family=Maven+Pro:wght@400..900&family=Montserrat:ital,wght@0,100..900;1,100..900&family=Open+Sans:ital,wght@0,300..800;1,300..800&display=swap"
      rel="stylesheet"
    >
   
    <link
      href="https://fonts.googleapis.com/css2?family=Maven+Pro:wght@400..900&family=Montserrat:ital,wght@0,100..900;1,100..900&family=Nunito:ital,wght@0,200..1000;1,200..1000&family=Open+Sans:ital,wght@0,300..800;1,300..800&display=swap"
      rel="stylesheet"
    >
    
    <link
      href="https://fonts.googleapis.com/css2?family=Maven+Pro:wght@400..900&family=Montserrat:ital,wght@0,100..900;1,100..900&family=Nunito:ital,wght@0,200..1000;1,200..1000&family=Open+Sans:ital,wght@0,300..800;1,300..800&family=Oswald:wght@200..700&display=swap"
      rel="stylesheet"
    >
   
    <title>
      {{ page_title }}
      {%- if current_tags %}
        &ndash; tagged "{{ current_tags | join: ', ' }}"
      {%- endif -%}
      {%- if current_page != 1 %}
        &ndash; Page {{ current_page -}}
      {%- endif -%}
      {%- unless page_title contains shop.name %}
        &ndash; {{ shop.name -}}
      {%- endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}
    <style>
      @font-face {
          font-family: "HarmoniaSansW01-Regular";         
          url("https://cdn.shopify.com/s/files/1/0888/6393/2702/files/0dd7edde115ef38e7def4b16e93b5884.woff2?v=1738679582")format("woff2"),
          url("https://cdn.shopify.com/s/files/1/0888/6393/2702/files/0dd7edde115ef38e7def4b16e93b5884.woff?v=1738679582")format("woff");
      }
      @font-face {
          font-family: 'Americana BT';
          src: url('https://cdn.shopify.com/s/files/1/0888/6393/2702/files/AmericanaBT-Bold.woff2?v=1734953497') format('woff2'),
              url('https://cdn.shopify.com/s/files/1/0888/6393/2702/files/AmericanaBT-Bold.woff?v=1734953497') format('woff');
          font-weight: bold;
          font-style: normal;
          font-display: swap;
      }
      
      @font-face {
          font-family: 'Americana BT';
          src: url('https://cdn.shopify.com/s/files/1/0888/6393/2702/files/AmericanaBT-Roman.woff2?v=1734953498') format('woff2'),
              url('https://cdn.shopify.com/s/files/1/0888/6393/2702/files/AmericanaBT-Roman.woff?v=1734953497') format('woff');
          font-weight: normal;
          font-style: normal;
          font-display: swap;
      }
      
      @font-face {
          font-family: 'Americana BT';
          src: url('https://cdn.shopify.com/s/files/1/0888/6393/2702/files/AmericanaBT-Italic.woff2?v=1734953497') format('woff2'),
              url('https://cdn.shopify.com/s/files/1/0888/6393/2702/files/AmericanaBT-Italic.woff?v=1734953497') format('woff');
          font-weight: normal;
          font-style: italic;
          font-display: swap;
      }


    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="{{ 'pubsub.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'global.js' | asset_url }}" defer="defer"></script>
    <link rel="stylesheet" href="{{ 'owl.carousel.min.css' | asset_url }}">
    <script src="{{ 'owl.carousel.min.js' | asset_url }}"></script>

    {{ content_for_header }}

    {%- render 'settings' -%}

    {{ 'base.css' | asset_url | stylesheet_tag }}
    {{ 'custom.css' | asset_url | stylesheet_tag }}
    {{ 'swiper-bundle.min.css' | asset_url | stylesheet_tag }}
    <link rel="stylesheet" href="{{ 'component-drawer.css' | asset_url }}" media="print" onload="this.media='all'">
    <noscript>
      {{ 'component-drawer.css' | asset_url | stylesheet_tag }}
    </noscript>

    {%- if settings.predictive_search_enabled -%}
      <link
        rel="stylesheet"
        href="{{ 'component-predictive-search.css' | asset_url }}"
        media="print"
        onload="this.media='all'"
      >
      <script src="{{- 'predictive-search.js' | asset_url -}}" defer="defer"></script>
      <noscript>
        {{- 'component-predictive-search.css' | asset_url | stylesheet_tag -}}
      </noscript>
    {%- endif -%}

    {% comment %}theme-check-disable AssetPreload{% endcomment %}
    {%- unless settings.type_heading_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_heading_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%}
    {%- unless settings.type_body_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_body_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%}
    {% comment %}theme-check-enable AssetPreload{% endcomment %}

    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');

      if (Shopify.designMode) {
        document.documentElement.classList.add('shopify-design-mode');
      }
    </script>

    <script src="{{- 'swiper-bundle.min.js' | asset_url -}}" defer="defer"></script>
    <script src="{{- 'bodyScrollLock.min.js' | asset_url -}}" defer="defer"></script>
    {%- if settings.image_snippet != 'native' -%}
      <script src="{{- 'lazysizes.min.js' | asset_url -}}" defer="defer"></script>
    {%- endif -%}
  </head>

  <body class="template template--{{ template.name }}{% if template.suffix %} template--{{- template.suffix -}}{% endif %} template-theme--{{- settings.theme_style_name -}}">
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{- 'accessibility.skip_to_text' | t -}}
    </a>

    {%- liquid
      section 'newsletter-popup'
      section 'age-verification-popup'

      sections 'header-group'

      section 'cart-drawer'
    -%}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% sections 'footer-group' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{- 'accessibility.refresh_page' | t -}}</li>
    </ul>

    <script>
      window.theme = window.theme || {};
      theme = {
        settings: {
          money_with_currency_format: {{ shop.money_format | json }},
          cart_free_shipping_threshold: {{ settings.shipping_notification_threshold | times: 100 }},
          cart_free_shipping_text: `{{ 'cart.shipping_bar.progress_html' | t: amount: '0' }}`,
          cart_free_shipping_text_success: `{{ 'cart.shipping_bar.success' | t }}`,
        }
      }
      window.shopUrl = '{{ request.origin }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}'
      };
      window.cartStrings = {
        error: `{{ 'cart.cart_error' | t }}`,
        quantityError: `{{ 'cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
      }
      window.variantStrings = {
        unavailable: '{{ 'products.product.unavailable' | t }}',
        addToCart: '{{ 'products.product.add_to_cart' | t }}',
        soldOut: '{{ 'products.product.sold_out' | t }}',
        addSubscriptionToCart: 'Add subscription to cart'
      }
      window.validationStrings = {
        invalidEmail: '{{ 'general.validation.invalid_email' | t }}'
      }
      window.accessibilityStrings = {
        recipientFormExpanded: `{{ 'recipient.form.expanded' | t }}`,
        recipientFormCollapsed: `{{ 'recipient.form.collapsed' | t }}`,
      };
    </script>

    {% if settings.enable_back_to_top_button %}
      {% render 'back-to-top' %}
    {% endif %}

    {%- comment -%}subify snippet start{%- endcomment -%}
    {% if content_for_header contains 'https:\/\/app-subify.shopgram.app\/subify-script-tag-for-snippet.js' %}
      {% render 'subify-snippet' %}
    {% endif %}
    {%- comment -%}subify snippet end{%- endcomment -%}
  </body>
</html>
